{
  "apps": [
    {
      "name": "bot-www-eating",
      "script": "./bin/www",
      "cwd": "./",
      "watch": [
        "./dist"
      ],
      "ignore_watch": [
        "node_modules",
        "public",
        "logs"
      ],
      "env": {
        "NODE_ENV": "dev"
      },
      "env_production": {
        "NODE_ENV": "prod"
      },
      "out_file": "./logs/out.log",
      "error_file": "./logs/err.log",
      "merge_logs": true,
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    }
  ]
}
